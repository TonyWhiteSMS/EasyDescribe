<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>Hierarchy</customSettingsType>
    <customSettingsVisibility>Protected</customSettingsVisibility>
    <description>PaymentConnect export page settings</description>
    <fields>
        <fullName>Bank_Account__c</fullName>
        <inlineHelpText>Bank account name to use for transactions not associated with a processor connection record.</inlineHelpText>
        <label>Bank Account</label>
        <length>255</length>
        <type>Text</type>
    </fields>
    <fields>
        <fullName>Export_Unassigned__c</fullName>
        <defaultValue>true</defaultValue>
        <inlineHelpText>Flag enabling export of transactions that have not been assigned to a processor connection record.</inlineHelpText>
        <label>Export Unassigned</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Fee_Account__c</fullName>
        <inlineHelpText>Quickbooks fee account name to use for transactions not associated with a processor connection record.</inlineHelpText>
        <label>Fee Account</label>
        <length>255</length>
        <type>Text</type>
    </fields>
    <fields>
        <fullName>Income_Account__c</fullName>
        <inlineHelpText>Quickbooks income account name to use for transactions not associated with a processor connection record.</inlineHelpText>
        <label>Income Account</label>
        <length>255</length>
        <type>Text</type>
    </fields>
    <fields>
        <fullName>Last_Export_End__c</fullName>
        <inlineHelpText>End date for last export.  Used to set the default start date for the next export.</inlineHelpText>
        <label>Last Export End</label>
        <type>Date</type>
    </fields>
    <fields>
        <fullName>Tax_Account__c</fullName>
        <defaultValue>&quot;Sales Tax Payable&quot;</defaultValue>
        <inlineHelpText>Quickbooks tax account name to use in exporting PaymentConnect transactions.</inlineHelpText>
        <label>Tax Account</label>
        <length>255</length>
        <type>Text</type>
    </fields>
    <label>Export_Page_Settings</label>
</CustomObject>
