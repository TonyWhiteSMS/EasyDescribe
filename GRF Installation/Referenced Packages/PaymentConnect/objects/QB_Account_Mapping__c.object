<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>List</customSettingsType>
    <customSettingsVisibility>Protected</customSettingsVisibility>
    <description>Names of Quickbook accounts mapped to Processor Connections in PaymentConnect</description>
    <fields>
        <fullName>Account_Name__c</fullName>
        <inlineHelpText>Quickbooks account name</inlineHelpText>
        <label>Account Name</label>
        <length>255</length>
        <type>Text</type>
    </fields>
    <fields>
        <fullName>Bank_Account__c</fullName>
        <inlineHelpText>Quickbooks bank account name to use in exporting PaymentConnect transactions.</inlineHelpText>
        <label>Bank Account</label>
        <length>255</length>
        <type>Text</type>
    </fields>
    <fields>
        <fullName>Connection_Id__c</fullName>
        <caseSensitive>true</caseSensitive>
        <inlineHelpText>Processor Connection record Id</inlineHelpText>
        <label>Connection Id</label>
        <length>18</length>
        <type>Text</type>
        <unique>true</unique>
    </fields>
    <fields>
        <fullName>Export__c</fullName>
        <defaultValue>true</defaultValue>
        <inlineHelpText>Enable export for this processor connection</inlineHelpText>
        <label>Export</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Fee_Account__c</fullName>
        <defaultValue>&quot;Other Expense&quot;</defaultValue>
        <inlineHelpText>Name of Quickbooks account for transaction fees</inlineHelpText>
        <label>Fee Account</label>
        <length>255</length>
        <type>Text</type>
    </fields>
    <fields>
        <fullName>Income_Account__c</fullName>
        <inlineHelpText>Name of Quickbooks account for income entry</inlineHelpText>
        <label>Income Account</label>
        <length>255</length>
        <type>Text</type>
    </fields>
    <fields>
        <fullName>Tax_Account__c</fullName>
        <inlineHelpText>Quickbooks tax account name to use in exporting PaymentConnect transactions.</inlineHelpText>
        <label>Tax Account</label>
        <length>255</length>
        <type>Text</type>
    </fields>
    <label>Quickbooks Account Mapping</label>
</CustomObject>
