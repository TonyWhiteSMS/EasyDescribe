    <validationRules>
        <fullName>Approval_Validation_for_Donation_Site</fullName>
        <active>true</active>
        <errorConditionFormula>and( Site_Closed__c=false, Event_Plan_Reviewed__c   , Site_Type__c  =&apos;Donation&apos; ,Or(
isnull(  Chapter_Contact_Email__c ),isnull( Event_Tagline__c ), isnull(  Event_Title__c  ) ,isnull( Event_Description__c ),isnull(  Support__c  ) ))</errorConditionFormula>
        <errorMessage>Not all fields have been completed for the Donation Page.  Please review the fields on the page (Chapter Contact Email, Tagline, Title, Description, Support).</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Approval_Validation_for_Event</fullName>
        <active>true</active>
        <errorConditionFormula>and( Site_Closed__c=false,  Event_Plan_Reviewed__c   , Site_Type__c  =&apos;Registration&apos; ,Or(
isnull( Event_Date__c ), isnull(  Chapter_Contact_Email__c ),isnull( Event_Tagline__c ), isnull(  Event_Title__c  ),  isnull( Event_Cost_How_Much__c ),  isnull(  Event_Location_Where__c ), isnull( Registration_Description__c ),isnull( For_more_information_contact__c ) ,isnull( Event_Description__c ),isnull( Event_Admission_Fee__c ) ))</errorConditionFormula>
        <errorMessage>Not all fields have been completed on the Event Page. Please review the fields on the page(Chapter Contact Email, Tagline, Title, Description, Support, Cost, Location, For more info, contact)..</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Blank_Chapter_Contact_Email</fullName>
        <active>true</active>
        <description>The Chapter Contact requires an e-mail address.</description>
        <errorConditionFormula>and(! Ispickval( Create_Site__c ,&apos;&apos;),isnull(Chapter_Contact_Email__c ))</errorConditionFormula>
        <errorMessage>The Chapter Contact requires an e-mail address.  Please add a Chapter Contact or update the contacts e-mail address.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Certification</fullName>
        <active>true</active>
        <errorConditionFormula>and (not isblank(Certification_Signature__c ), isnull( Certification_Date__c ))</errorConditionFormula>
        <errorDisplayField>Certification_Date__c</errorDisplayField>
        <errorMessage>Please enter a certification date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Chapter_Contact_Completion</fullName>
        <active>true</active>
        <errorConditionFormula>and(ispickval( Create_Site__c,&apos;&apos;),isnull( Chapter_Contact__c ))</errorConditionFormula>
        <errorMessage>The fundraising site must have a Chapter Contact assigned.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Chapter_Logo_Scaling_Field</fullName>
        <active>true</active>
        <errorConditionFormula>If(isnull (  Chapter_Logo_URL__c )  ,false   ,If(isnull (Chapter_Logo_Height__c)= isnull( Chapter_Logo_Width__c ),false,true))</errorConditionFormula>
        <errorDisplayField>Chapter_Logo_Height__c</errorDisplayField>
        <errorMessage>Please check your scaling values for the Chapter Logo.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Either_Flickr_or_Event_Picture</fullName>
        <active>true</active>
        <errorConditionFormula>and( not isblank(Event_Picture_URL__c) , not isblank( Flickr_User_Id__c ))</errorConditionFormula>
        <errorDisplayField>Flickr_User_Id__c</errorDisplayField>
        <errorMessage>Due to space limitations, displaying a Flickr requires that the Event Photo be removed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Event_Photo_Scaling_Field</fullName>
        <active>true</active>
        <errorConditionFormula>If(isnull (Event_Picture_Height__c)= isnull( Event_Picture_Width__c ),
If(Event_Picture_Width__c &gt;250,true, false),true )</errorConditionFormula>
        <errorDisplayField>Chapter_Logo_Height__c</errorDisplayField>
        <errorMessage>Please check your scaling values for the Event photo.  The width should not exceed 250 pixels.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Site_closed_prior_to_certification</fullName>
        <active>true</active>
        <errorConditionFormula>and (not isblank(Certification_Signature__c ), ! Site_Closed__c )</errorConditionFormula>
        <errorMessage>The site must be closed prior to certification.</errorMessage>
    </validationRules>
</CustomObject>
